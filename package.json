{
  "name": "@aslamhus/fileicon",
  "version": "1.0.0",
  "description": "Create a file icon image with any file extension",
  "main": "./dist/FileIcon.js",
  "scripts": {
    "prebuild": "npx babel src/FileIcon.js --out-file dist/FileIcon.mjs",
    "build": "npx babel src/FileIcon.js --out-file dist/FileIcon.js --presets @babel/preset-env --plugins @babel/plugin-transform-modules-commonjs",
    "postbuild": "npx babel src/FileIcon.js --out-file lib/FileIcon-lib.js --presets @babel/preset-env --plugins @babel/plugin-transform-modules-umd",
    "test": "mocha"
  },
  "author": "Aslam Husain <aslam.percy.husain@gmail.com> (https://github.com/aslamhus)",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.6",
    "@babel/plugin-proposal-optional-chaining": "^7.14.5",
    "@babel/plugin-transform-modules-commonjs": "^7.14.5",
    "@babel/plugin-transform-modules-umd": "^7.14.5",
    "@babel/preset-env": "^7.14.7",
    "canvas": "^2.8.0",
    "chai": "^4.3.4",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-node": "^4.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "jsdom": "^16.6.0",
    "mocha": "^9.0.2"
  }
}
